# Example environment file for TimelineVerse project
# Copy to .env and adjust as needed for your environment

# SenseNet repository root path (default: /Root/Content)
VITE_PROJECT_ROOT_PATH=/Root/Content

# Relative paths to API keys in SenseNet ECM
VITE_OMDB_KEY_PATH=/(structure)/apikeys/omdb
VITE_TMDB_KEY_PATH=/(structure)/apikeys/tmdb
VITE_TRAKT_KEY_PATH=/(structure)/apikeys/trakt

# SenseNet repository URL
VITE_SENSENET_REPO_URL=https://your-sensenet-repo-url

# OIDC Identity Server Configuration
VITE_OIDC_AUTHORITY=https://your-identity-server-url

# Use local test Trakt list JSON for development (set to 'true' to enable)
# VITE_USE_TEST_TRAKT_LIST=true